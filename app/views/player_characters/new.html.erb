<h1>Create a New Character</h1>

<%= form_for :player_character, url: player_characters_path do |f| %>
  <p>
    <%= f.label :character_name %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :Physical_description %><br>
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.label :character_backstory %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>